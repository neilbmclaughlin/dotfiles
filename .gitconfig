[alias]
	st = status
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
	co = checkout
	cob = checkout -b
	ci = commit -m
	b = branch
	com = checkout master
	aa = add -A
  d = diff
  fpwl = push --force-with-lease
	alias = ! git config --get-regexp ^alias\\. | sed -e s/^alias\\.// -e s/\\ /\\ =\\ /
[user]
	name = neil mclaughlin
	email = neil.mclaughlin@defra.gov.uk
[color]
	ui = true
[push]
	default = upstream
[core]
	editor = vim
	excludesfile = /Users/neilmclaughlin/.gitignore
[credential]
	helper = osxkeychain
[github]
	user = neilbmclaughlin
[commit]
	verbose = true
[pull]
	ff = only
