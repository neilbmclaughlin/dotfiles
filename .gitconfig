[alias]
	st = status
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  sw = switch
  swc = switch -c
	swm = switch master
	cm = commit -m
	b = branch
	aa = add -A
  d = diff
  fpwl = push --force-with-lease
	alias = ! git config --get-regexp ^alias\\. | sed -e s/^alias\\.// -e s/\\ /\\ =\\ /
[user]
	name = neil mclaughlin
	email = neil.mclaughlin@defra.gov.uk
[color]
	ui = true
[push]
	default = upstream
	autoSetupRemote = true
[core]
	editor = vim
	excludesfile = /Users/neil/.gitignore
[credential]
	helper = osxkeychain
[github]
	user = neilbmclaughlin
[commit]
	verbose = true
[pull]
	ff = only
[init]
	defaultBranch = main
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
